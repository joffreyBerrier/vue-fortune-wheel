{
  "name": "vue3-fortune-wheel",
  "version": "2.1.2",
  "description": "A Vue 3 Fortune Wheel component",
  "repository": "https://github.com/joffreyBerrier/vue3-fortune-wheel",
  "funding": "https://github.com/sponsors/joffreyBerrier",
  "license": "MIT",
  "author": "Joffrey Berrier <joffrey.berrier@gmail.com>",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/fortune-wheel.umd.js",
  "module": "./dist/fortune-wheel.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/fortune-wheel.es.js",
      "require": "./dist/fortune-wheel.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue3",
    "typescript",
    "fortune-wheel",
    "vue-fortune-wheel",
    "wheel-of-fortune"
  ],
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=20.14.0",
    "pnpm": ">=9.5.0"
  },
  "packageManager": "pnpm@9.5.0"
}
